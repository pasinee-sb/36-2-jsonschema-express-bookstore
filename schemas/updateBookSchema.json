{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "amazon_url",
        "author",
        "language",
        "pages",
        "publisher",
        "title",
        "year"
    ],
    "properties": {
        "isbn": {
            "type": "string",
            "read-only": true,
            "default": "",
            "title": "The isbn Schema",
            "examples": [
                "czsc"
            ]
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "scsc"
            ]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "scsc"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "scvsxc"
            ]
        },
        "pages": {
            "type": "number",
            "default": "",
            "title": "The pages Schema",
            "examples": [
                "15"
            ]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "svca"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "scsc"
            ]
        },
        "year": {
            "type": "number",
            "default": "",
            "title": "The year Schema",
            "examples": [
                "1990"
            ]
        }
    },
    "examples": [{
        "isbn":"dgda",
        "amazon_url": "scsc",
        "author": "scsc",
        "language": "scvsxc",
        "pages": 15,
        "publisher": "svca",
        "title": "scsc",
        "year": 1990
    }]
}